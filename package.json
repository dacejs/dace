{
  "name": "dace",
  "version": "1.6.0",
  "description": "A minimalistic universal (isomorphic) react framework for building web apps",
  "main": "dist/index.js",
  "repository": "git@github.com:dacejs/dace.git",
  "homepage": "https://dacejs.github.io",
  "scripts": {
    "build": "NODE_ENV=local babel src -d dist --presets=@babel/preset-env,babel-preset-dace",
    "build:watch": "npm run build -- --watch",
    "postrelease": "git push && git push origin --tags",
    "prepare": "npm run build",
    "release": "standard-version && npm publish",
    "release:minor": "standard-version --release-as minor && npm publish",
    "release:major": "standard-version --release-as major && npm publish",
    "test": "node test/index.js",
    "test:debug": "DEBUG=true node test/index.js",
    "pretest": "npm run build"
  },
  "bin": {
    "dace": "./dist/bin/dace.js"
  },
  "keywords": [
    "universal",
    "isomorphic",
    "webpack",
    "react",
    "redux",
    "server-side-rendering",
    "express",
    "code-splitting"
  ],
  "files": [
    "dist",
    ".eslintrc.js"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@loadable/component": "^5.2.2",
    "@loadable/server": "^5.2.2",
    "@loadable/webpack-plugin": "^5.2.2",
    "babel-loader": "^8.0.0",
    "babel-preset-dace": "^2.0.2",
    "chalk": "^2.4.1",
    "clean-webpack-plugin": "^0.1.19",
    "commander": "^2.18.0",
    "css-loader": "^1.0.0",
    "dace-plugin-redux": "^1.0.12",
    "dotenv": "^6.0.0",
    "eslint": "^5.6.0",
    "eslint-config-qunar": "^4.0.0",
    "eslint-loader": "^2.1.1",
    "file-loader": "^1.1.11",
    "import-fresh": "^2.0.0",
    "mini-css-extract-plugin": "^0.4.3",
    "packing-urlrewrite": "^0.2.0",
    "postcss-loader": "^2.1.6",
    "postcss-preset-env": "^6.0.6",
    "postcss-reporter": "^6.0.0",
    "prop-types": "^15.6.2",
    "qs": "^6.5.2",
    "react-dev-utils": "^4.0.0",
    "react-error-overlay": "^2.0.0",
    "react-helmet": "^5.2.0",
    "react-router-config": "^1.0.0-beta.4",
    "react-router-dom": "^4.3.1",
    "start-server-webpack-plugin": "2.2.5",
    "style-loader": "0.20.3",
    "stylelint": "^9.3.0",
    "stylelint-config-dace": "^1.0.0",
    "url-loader": "^1.0.1",
    "webpack": "^4.20.2",
    "webpack-dev-server-speedy": "^3.1.1",
    "webpack-node-externals": "^1.7.2",
    "webpack-visualizer-plugin": "^0.1.11",
    "webpackbar": "^2.6.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "mocha": "^5.2.0",
    "ps-tree": "^1.1.0",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "shelljs": "^0.8.2",
    "should": "^13.2.1"
  }
}
