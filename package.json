{
  "name": "dace",
  "version": "1.6.0",
  "description": "A minimalistic universal (isomorphic) react framework for building web apps",
  "main": "dist/index.js",
  "repository": "git@github.com:dacejs/dace.git",
  "homepage": "https://dacejs.com",
  "scripts": {
    "build": "NODE_ENV=local babel src -d dist --presets=env,dace",
    "build:watch": "npm run build -- --watch",
    "postrelease": "git push && git push origin --tags",
    "prepare": "npm run build",
    "release": "standard-version && npm publish",
    "test": "node test/index.js",
    "test:debug": "DEBUG=true node test/index.js",
    "pretest": "npm run build"
  },
  "bin": {
    "dace": "./dist/bin/dace.js"
  },
  "keywords": [
    "universal",
    "isomorphic",
    "webpack",
    "react",
    "redux",
    "server-side-rendering",
    "koa",
    "code-splitting"
  ],
  "files": [
    "dist",
    ".eslintrc.js"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-preset-dace": "^1.0.7",
    "babel-preset-env": "^1.7.0",
    "chalk": "^2.4.1",
    "clean-webpack-plugin": "^1.0.0",
    "commander": "^2.19.0",
    "css-loader": "^2.1.0",
    "dotenv": "^6.2.0",
    "eslint": "^5.12.0",
    "eslint-config-qunar": "^4.0.0",
    "eslint-loader": "^2.1.1",
    "file-loader": "^3.0.1",
    "import-fresh": "^3.0.0",
    "mini-css-extract-plugin": "^0.5.0",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.5.0",
    "postcss-reporter": "^6.0.1",
    "prop-types": "^15.6.2",
    "qs": "^6.6.0",
    "react-dev-utils": "^4.0.0",
    "react-error-overlay": "^2.0.0",
    "react-helmet": "^5.2.0",
    "react-router-config": "^1.0.0-beta.4",
    "react-router-dom": "^4.3.1",
    "start-server-webpack-plugin": "2.2.5",
    "style-loader": "^0.23.1",
    "stylelint": "^9.9.0",
    "stylelint-config-dace": "^1.0.0",
    "url-loader": "^1.1.2",
    "webpack": "^4.28.3",
    "webpack-dev-server-speedy": "^3.1.1",
    "webpack-node-externals": "^1.7.2",
    "webpack-visualizer-plugin": "^0.1.11",
    "webpackbar": "^2.6.4"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "mocha": "^5.2.0",
    "ps-tree": "^1.2.0",
    "shelljs": "^0.8.3",
    "should": "^13.2.3"
  }
}
